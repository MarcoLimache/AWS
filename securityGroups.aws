### Obtener SG asociados a un CIDR
aws ec2 describe-security-groups --filters Name=ip-permission.cidr,Values='x.x.x.x/x' --query "SecurityGroups[*].[*]" --output text

### Describir un único SG
aws ec2 describe-security-groups --query "SecurityGroups[*].[*]" --filter Name=group-name,Values='SG-NAME' --output json --profile vpc2

### Crear reglas en un único SG
aws ec2 authorize-security-group-ingress --group-id sg-xxxxxxxxxxx --ip-permissions IpProtocol=udp,FromPort=1812,ToPort=1812,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=udp,FromPort=1813,ToPort=1813,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=udp,FromPort=3799,ToPort=3799,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=tcp,FromPort=3799,ToPort=3799,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=udp,FromPort=6658,ToPort=6658,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=tcp,FromPort=443,ToPort=443,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=tcp,FromPort=5432,ToPort=5432,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=udp,FromPort=123,ToPort=123,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] IpProtocol=icmp,FromPort=-1,ToPort=-1,IpRanges=[{CidrIp=x.x.x.x/x,Description="DESCRIPCION"}] --profile vpc2

